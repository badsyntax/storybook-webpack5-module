{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2020",
    "rootDir": "./src",
    "outDir": "./lib",
    "strict": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "declaration": true,
    "jsx": "react-jsx",
    "sourceMap": true
  },
  "exclude": ["node_modules", "lib"]
}
